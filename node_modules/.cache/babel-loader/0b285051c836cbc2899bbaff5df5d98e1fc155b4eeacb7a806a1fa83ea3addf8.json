{"ast":null,"code":"export default {\n  data() {\n    return {\n      formData: {\n        name: '',\n        email: '',\n        subject: '',\n        message: ''\n      },\n      successMessage: '',\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      const apiUrl = 'https://app.dropshiphub.nl/api/v1/contact';\n      console.log('Submitting to:', apiUrl);\n      try {\n        const response = await fetch(apiUrl, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n            'Accept': 'application/json'\n          },\n          body: JSON.stringify({\n            email: this.formData.email,\n            firstname: this.formData.name,\n            subject: this.formData.subject,\n            message: this.formData.message\n          })\n        });\n        if (response.ok) {\n          this.successMessage = 'Message sent successfully!';\n          this.errorMessage = '';\n          this.formData = {\n            name: '',\n            email: '',\n            subject: '',\n            message: ''\n          };\n          setTimeout(() => {\n            this.successMessage = '';\n          }, 5000);\n        } else {\n          const errorData = await response.json();\n          throw new Error(errorData.message || 'Failed to send message');\n        }\n      } catch (error) {\n        this.errorMessage = error.message || 'Failed to send message. Please try again.';\n        this.successMessage = '';\n        setTimeout(() => {\n          this.errorMessage = '';\n        }, 5000);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","formData","name","email","subject","message","successMessage","errorMessage","methods","submitForm","apiUrl","console","log","response","fetch","method","headers","body","JSON","stringify","firstname","ok","setTimeout","errorData","json","Error","error"],"sources":["/Users/marwanaboudaoud/projects/DropshiphubWebsite/src/components/contact/contact-one.vue"],"sourcesContent":["<template>\r\n    <section class=\"relative lg:py-24 py-16\">\r\n        <div class=\"container\">\r\n            <div class=\"grid md:grid-cols-12 grid-cols-1 items-center gap-[30px]\">\r\n                <div class=\"lg:col-span-7 md:col-span-6\">\r\n                    <div class=\"lg:me-5\">\r\n                        <div class=\"bg-white dark:bg-slate-900 rounded-md shadow dark:shadow-gray-800 p-6\">\r\n                            <h3 class=\"mb-6 text-2xl leading-normal font-medium\">Get in touch !</h3>\r\n\r\n                            <form @submit.prevent=\"submitForm\">\r\n                                <div class=\"grid lg:grid-cols-12 lg:gap-6\">\r\n                                    <div class=\"lg:col-span-6 mb-5\">\r\n                                        <div class=\"text-start\">\r\n                                            <label for=\"name\" class=\"font-semibold\">Your Name:</label>\r\n                                            <input v-model=\"formData.name\" type=\"text\" id=\"name\" class=\"form-input w-full text-[15px] py-2 px-3 h-10 bg-transparent dark:bg-slate-900 dark:text-slate-200 rounded outline-none border border-gray-200 focus:border-indigo-600 dark:border-gray-800 dark:focus:border-indigo-600 focus:ring-0 mt-2\" placeholder=\"Name :\">\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div class=\"lg:col-span-6 mb-5\">\r\n                                        <div class=\"text-start\">\r\n                                            <label for=\"email\" class=\"font-semibold\">Your Email:</label>\r\n                                            <input v-model=\"formData.email\" type=\"email\" id=\"email\" class=\"form-input w-full text-[15px] py-2 px-3 h-10 bg-transparent dark:bg-slate-900 dark:text-slate-200 rounded outline-none border border-gray-200 focus:border-indigo-600 dark:border-gray-800 dark:focus:border-indigo-600 focus:ring-0 mt-2\" placeholder=\"Email :\">\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <div class=\"grid grid-cols-1\">\r\n                                    <div class=\"mb-5\">\r\n                                        <div class=\"text-start\">\r\n                                            <label for=\"subject\" class=\"font-semibold\">Your Question:</label>\r\n                                            <input v-model=\"formData.subject\" type=\"text\" id=\"subject\" class=\"form-input w-full text-[15px] py-2 px-3 h-10 bg-transparent dark:bg-slate-900 dark:text-slate-200 rounded outline-none border border-gray-200 focus:border-indigo-600 dark:border-gray-800 dark:focus:border-indigo-600 focus:ring-0 mt-2\" placeholder=\"Subject :\">\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div class=\"mb-5\">\r\n                                        <div class=\"text-start\">\r\n                                            <label for=\"message\" class=\"font-semibold\">Your Message:</label>\r\n                                            <textarea v-model=\"formData.message\" id=\"message\" class=\"form-input w-full text-[15px] py-2 px-3 h-28 bg-transparent dark:bg-slate-900 dark:text-slate-200 rounded outline-none border border-gray-200 focus:border-indigo-600 dark:border-gray-800 dark:focus:border-indigo-600 focus:ring-0 mt-2\" placeholder=\"Message :\"></textarea>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                \r\n                                <div v-if=\"successMessage\" class=\"p-4 mb-4 text-sm text-green-700 bg-green-100 rounded-lg dark:bg-green-200 dark:text-green-800\" role=\"alert\">\r\n                                    {{ successMessage }}\r\n                                </div>\r\n                                \r\n                                <div v-if=\"errorMessage\" class=\"p-4 mb-4 text-sm text-red-700 bg-red-100 rounded-lg dark:bg-red-200 dark:text-red-800\" role=\"alert\">\r\n                                    {{ errorMessage }}\r\n                                </div>\r\n\r\n                                <button type=\"submit\" class=\"py-2 px-5 inline-block font-semibold tracking-wide border align-middle duration-500 text-base text-center bg-dropshiphub-primary-500 hover:bg-dropshiphub-primary-600 border-dropshiphub-primary-500 hover:border-dropshiphub-primary-600 text-white rounded-md\">Send Message</button>\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            formData: {\r\n                name: '',\r\n                email: '',\r\n                subject: '',\r\n                message: ''\r\n            },\r\n            successMessage: '',\r\n            errorMessage: ''\r\n        }\r\n    },\r\n    methods: {\r\n        async submitForm() {\r\n        const apiUrl = 'https://app.dropshiphub.nl/api/v1/contact'\r\n        console.log('Submitting to:', apiUrl) \r\n        \r\n        try {\r\n            const response = await fetch(apiUrl, {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application/json',\r\n                    'Accept': 'application/json'\r\n                },\r\n                body: JSON.stringify({\r\n                    email: this.formData.email,\r\n                    firstname: this.formData.name,\r\n                    subject: this.formData.subject,\r\n                    message: this.formData.message\r\n                })\r\n            })\r\n            \r\n            if (response.ok) {\r\n                this.successMessage = 'Message sent successfully!'\r\n                this.errorMessage = ''\r\n                this.formData = {\r\n                    name: '',\r\n                    email: '',\r\n                    subject: '',\r\n                    message: ''\r\n                }\r\n                setTimeout(() => {\r\n                    this.successMessage = ''\r\n                }, 5000)\r\n            } else {\r\n                const errorData = await response.json()\r\n                throw new Error(errorData.message || 'Failed to send message')\r\n            }\r\n        } catch (error) {\r\n            this.errorMessage = error.message || 'Failed to send message. Please try again.'\r\n            this.successMessage = ''\r\n            setTimeout(() => {\r\n                this.errorMessage = ''\r\n            }, 5000)\r\n        }\r\n    }\r\n}\r\n\r\n\r\n}\r\n</script>\r\n"],"mappings":"AA6DA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACb,CAAC;MACDC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAClB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACnB,MAAMC,MAAK,GAAI,2CAA0C;MACzDC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,MAAM;MAEpC,IAAI;QACA,MAAMG,QAAO,GAAI,MAAMC,KAAK,CAACJ,MAAM,EAAE;UACjCK,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACL,cAAc,EAAE,kBAAkB;YAClC,QAAQ,EAAE;UACd,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YACjBhB,KAAK,EAAE,IAAI,CAACF,QAAQ,CAACE,KAAK;YAC1BiB,SAAS,EAAE,IAAI,CAACnB,QAAQ,CAACC,IAAI;YAC7BE,OAAO,EAAE,IAAI,CAACH,QAAQ,CAACG,OAAO;YAC9BC,OAAO,EAAE,IAAI,CAACJ,QAAQ,CAACI;UAC3B,CAAC;QACL,CAAC;QAED,IAAIQ,QAAQ,CAACQ,EAAE,EAAE;UACb,IAAI,CAACf,cAAa,GAAI,4BAA2B;UACjD,IAAI,CAACC,YAAW,GAAI,EAAC;UACrB,IAAI,CAACN,QAAO,GAAI;YACZC,IAAI,EAAE,EAAE;YACRC,KAAK,EAAE,EAAE;YACTC,OAAO,EAAE,EAAE;YACXC,OAAO,EAAE;UACb;UACAiB,UAAU,CAAC,MAAM;YACb,IAAI,CAAChB,cAAa,GAAI,EAAC;UAC3B,CAAC,EAAE,IAAI;Q