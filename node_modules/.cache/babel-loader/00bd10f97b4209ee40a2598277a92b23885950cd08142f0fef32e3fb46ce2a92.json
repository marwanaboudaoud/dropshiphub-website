{"ast":null,"code":"export default {\n  data() {\n    return {\n      formData: {\n        name: '',\n        email: '',\n        subject: '',\n        message: ''\n      },\n      successMessage: '',\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await fetch('https://app.dropshiphub.nl/api/v1/contact', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(this.formData)\n        });\n        if (response.ok) {\n          this.successMessage = 'Message sent successfully!';\n          this.errorMessage = '';\n          // Clear form\n          this.formData = {\n            name: '',\n            email: '',\n            subject: '',\n            message: ''\n          };\n          // Clear success message after 5 seconds\n          setTimeout(() => {\n            this.successMessage = '';\n          }, 5000);\n        } else {\n          throw new Error('Failed to send message');\n        }\n      } catch (error) {\n        this.errorMessage = 'Failed to send message. Please try again.';\n        this.successMessage = '';\n        // Clear error message after 5 seconds\n        setTimeout(() => {\n          this.errorMessage = '';\n        }, 5000);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","formData","name","email","subject","message","successMessage","errorMessage","methods","submitForm","response","fetch","method","headers","body","JSON","stringify","ok","setTimeout","Error","error"],"sources":["/Users/marwanaboudaoud/projects/DropshiphubWebsite/src/components/contact/contact-one.vue"],"sourcesContent":["<template>\r\n    <section class=\"relative lg:py-24 py-16\">\r\n        <div class=\"container\">\r\n            <div class=\"grid md:grid-cols-12 grid-cols-1 items-center gap-[30px]\">\r\n                <div class=\"lg:col-span-7 md:col-span-6\">\r\n                    <div class=\"l